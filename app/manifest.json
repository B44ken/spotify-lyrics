{
	"icons": {
		"16" : "icon/16.png",
		"48" : "icon/48.png",
		"128": "icon/128.png"
	},
  "manifest_version": 2,
  "name": "Spotify Lyrics",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "https://*.spotify.com/*"
      ],
      "js": ["app.js"]
    }
  ]
}
